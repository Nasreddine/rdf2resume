          this.renderLabel(\\n                    translatedPropsOrg,\\n                    \\\"organizationName\\\",\\n                    lang\\n                  ) + \\\" *\\\"\\n                }\\n                type=\\\"text\\\"\\n                value={organizationName}\\n                handleChange={this.handleInputChange}\\n              />\\n            </Col>\\n          </Row>\\n          <div>\\n            <CustomInput\\n              id=\\\"my0:street\\\"\\n              name=\\\"address\\\"\\n              label={this.renderLabel(translatedPropsAddr, \\\"street\\\", lang)}\\n              type=\\\"text\\\"\\n              value={this.findTranslatedValue(address[\\\"my0:street\\\"], lang)}\\n              handleChange={(e) => this.handleInputChange(e, lang)}\\n            />\\n            <Row>\\n              <Col md={6}>\\n                <CustomInput\\n                  id=\\\"my0:postalCode\\\"\\n                  label={this.renderLabel(\\n                    translatedPropsAddr,\\n                    \\\"postalCode\\\",\\n                    lang\\n                  )}\\n                  name=\\\"address\\\"\\n                  type=\\\"text\\\"\\n                  value={address[\\\"my0:postalCode\\\"]}\\n                  handleChange={this.handleInputChange}\\n                />\\n              </Col>\\n              <Col md={6}>\\n                <CustomInput\\n                  id=\\\"my0:city\\\"\\n                  name=\\\"address\\\"\\n                  label={this.renderLabel(translatedPropsAddr, \\\"city\\\", lang)}\\n                  type=\\\"text\\\"\\n                  value={this.findTranslatedValue(address[\\\"my0:city\\\"], lang)}\\n                  handleChange={(e) => this.handleInputChange(e, lang)}\\n                />\\n              </Col>\\n            </Row>\\n            <Row\\n              style={{\\n                width: \\\"100%\\\",\\n                justifyContent: \\\"left\\\",\\n                marginLeft: