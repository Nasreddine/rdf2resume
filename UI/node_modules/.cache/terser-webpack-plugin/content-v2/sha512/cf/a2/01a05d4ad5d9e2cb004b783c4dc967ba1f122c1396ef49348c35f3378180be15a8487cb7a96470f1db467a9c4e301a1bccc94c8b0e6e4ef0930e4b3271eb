\\\", lang) + \\\" *\\\"\\n                  }\\n                  type=\\\"text\\\"\\n                  value={this.findTranslatedValue(jobTitle, lang)}\\n                  handleChange={(e) => this.handleInputChange(e, lang)}\\n                />\\n                <Row\\n                  style={{\\n                    width: \\\"100%\\\",\\n                    justifyContent: \\\"left\\\",\\n                    marginLeft: \\\"0px\\\",\\n                    marginBottom: \\\"8px\\\",\\n                    marginTop: \\\"5px\\\"\\n                  }}\\n                >\\n                  <label className=\\\"label-rw\\\">\\n                    {this.renderLabel(translatedProps, \\\"jobType\\\", lang)}\\n                  </label>\\n                  <Combobox\\n                    name=\\\"my0:jobType\\\"\\n                    placeholder={this.renderLabel(\\n                      translatedProps,\\n                      \\\"jobType\\\",\\n                      lang\\n                    )}\\n                    data={this.props.jobtypes}\\n                    textField={lang}\\n                    valueField=\\\"@type\\\"\\n                    value={jobType}\\n                    caseSensitive={false}\\n                    minLength={3}\\n                    filter=\\\"contains\\\"\\n                    onChange={value =>\\n                      this.handleSelectChange(\\n                        value,\\n                        \\\"my0:jobType\\\",\\n                        \\\"workHistory\\\"\\n                      )\\n                    }\\n                  />\\n                </Row>\\n                <Row\\n                  style={{\\n                    width: \\\"100%\\\",\\n                    justifyContent: \\\"left\\\",\\n                    marginLeft: \\\"0px\\\",\\n                    marginBottom: \\\"8px\\\"\\n                  }}\\n                >\\n                  <label className=\\\"label-rw\\\">\\n                    {this.renderLa