={name}\\n        as=\\\"textarea\\\"\\n        rows={rows ? rows : \\\"3\\\"}\\n        value={value}\\n        onChange={e => handleChange(e)}\\n      />\\n    </Form.Group>\\n  );\\n};\\n\\nexport default CustomTextarea;\\n\",\"import React from \\\"react\\\";\\nimport Form from \\\"react-bootstrap/Form\\\";\\nimport \\\"./Form.css\\\";\\n\\nconst CustomInput = (props) => {\\n  return (\\n    <Form.Group controlId={props.id} className=\\\"floating-label mb-2\\\">\\n      \\n      <Form.Control\\n        className={props.classnames}\\n        type={props.type}\\n        placeholder={props.label}\\n        value={props.value}\\n        name={props.name}\\n        onBlur={e => props.handleBlurEvent? props.handleBlurEvent(e) : ''}\\n        onChange={e => props.handleChange(e)}\\n      >\\n      </Form.Control>\\n      <Form.Label>{props.label}</Form.Label>\\n      {props.children}\\n    </Form.Group>\\n  );\\n};\\n\\nexport default CustomInput;\\n\",\"import React from \\\"react\\\";\\nimport Form from \\\"react-bootstrap/Form\\\";\\nimport \\\"./Form.css\\\";\\n\\nconst CustomCheckbox = ({\\n  id,\\n  name,\\n  type,\\n  label,\\n  classnames,\\n  handleChange,\\n  checked\\n}) => {\\n  let classes = \\\"form-check-input \\\" + classnames;\\n  let divClasses = \\\"\\\";\\n  if (label === undefined) {\\n    divClasses = \\\"form-check no-label\\\";\\n  } else {\\n    divClasses = \\\"form-check\\\";\\n  }\\n  return (\\n    <Form.Group style={{ marginBottom: \\\"0\\\" }}>\\n      <div className={divClasses} style={{ marginBottom: \\\"0\\\" }}>\\n        <label\\n          title={label}\\n          type={type}\\n          htmlFor={id}\\n          className=\\\"form-check-label\\\"\\n        >\\n          {label}\\n          <input\\n            name={name}\\n            type={type}\\n            id={id}\\n            className={classes}\\n            onChange={e => handleChange(e)}\\n            checked={checked}\\n          />\\n          <span className=\\\"checkma