ENCE';\\nexport const UPDATE_REFERENCE = 'UPDATE_REFERENCE';\\nexport const REMOVE_REFERENCE = 'REMOVE_REFERENCE';\\n\\n/* SKILL ACTION CREATORS */\\nexport const CREATE_OTHER_SKILL = 'CREATE_OTHER_SKILL';\\nexport const UPDATE_OTHER_SKILL = 'UPDATE_OTHER_SKILL';\\nexport const REMOVE_OTHER_SKILL = 'REMOVE_OTHER_SKILL';\\n\\n/* EDUCATION ACTION CREATORS */\\nexport const CREATE_EDUCATION = 'CREATE_EDUCATION';\\nexport const UPDATE_EDUCATION = 'UPDATE_EDUCATION';\\nexport const REMOVE_EDUCATION = 'REMOVE_EDUCATION';\\n\\n/* COURSES ACTION CREATORS */\\nexport const CREATE_COURSE = 'CREATE_COURSE';\\nexport const UPDATE_COURSE = 'UPDATE_COURSE';\\nexport const REMOVE_COURSE = 'REMOVE_COURSE';\\n\\n/* WORKHISTORY ACTION CREATORS */\\nexport const CREATE_WORK_HISTORY = 'CREATE_WORK_HISTORY';\\nexport const UPDATE_WORK_HISTORY = 'UPDATE_WORK_HISTORY';\\nexport const REMOVE_WORK_HISTORY = 'REMOVE_WORK_HISTORY';\\n\\n/* INSTANT MESSAGING ACTION CREATORS */\\nexport const CREATE_IM = 'CREATE_IM';\\nexport const UPDATE_IM = 'UPDATE_IM';\\nexport const REMOVE_IM = 'REMOVE_IM';\\n\\n/* WEBSITE ACTION CREATORS */\\nexport const CREATE_WEBSITE = 'CREATE_WEBSITE';\\nexport const UPDATE_WEBSITE = 'UPDATE_WEBSITE';\\nexport const REMOVE_WEBSITE = 'REMOVE_WEBSITE';\\n\\n/* PUBLICATION ACTION CREATORS */\\nexport const CREATE_PUBLICATION = 'CREATE_PUBLICATION';\\nexport const UPDATE_PUBLICATION = 'UPDATE_PUBLICATION';\\nexport const REMOVE_PUBLICATION = 'REMOVE_PUBLICATION';\\n\\n/* PATENT ACTION CREATORS */\\nexport const CREATE_PATENT = 'CREATE_PATENT';\\nexport const UPDATE_PATENT = 'UPDATE_PATENT';\\nexport const REMOVE_PATENT = 'REMOVE_PATENT';\\n\\n/* PROJECT ACTION CREATORS */\\nexport const CREATE_PROJECT = 'CREATE_PROJECT';\\nexport const UPDATE_PROJECT = 'UPDATE_PROJECT';\\nexport const REMOVE_PROJECT = 'REMOVE_PROJECT';\\n\\n/* HONOR ACTION CREATORS */\\nexport const CREATE_HONOR = 'CREATE_HONOR';\\nexport const UPDATE_HONOR = 'UPDATE_HONOR';\\nexport const REMOVE_HONOR = 