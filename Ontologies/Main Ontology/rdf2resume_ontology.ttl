my0:skillDescription
  a owl:DatatypeProperty ;
  rdfs:comment "Description of the skill"@en ;
  rdfs:label "skill Description"@en ;
  rdfs:domain my0:Skill ;
  rdfs:range xsd:string .

my0:CommunicationSkill
  a owl:Class ;
