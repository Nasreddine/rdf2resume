@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <https://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix ns0: <http://protege.stanford.edu/system#> .
@prefix country: <http://www.bpiresearch.com/BPMO/2004/03/03/cdl/Countries#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix dbo: <http://dbpedia.org/resource/classes#> .
@prefix esco: <http://data.europa.eu/esco/model> .
@prefix my0: <http://example.com/rdf2resume_ontology.rdf#> .
@prefix mybase0: <http://example.com/rdf2resume_base_ontology.rdf#> .

my0:
  a owl:Ontology ;
  rdfs:label "Curriculum Vitae Ontology"@en ;
  dc:issued "2019-05-15"^^xsd:date ;
  dc:modified "2019-06-13"^^xsd:date . 

my0:CV
  a owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty my0:cvLastUpdate ;
                    owl:onProperty my0:cvTitle ;
                    owl:onProperty my0:cvNotes ;
                    owl:onProperty my0:cvCopyright ;
                    owl:onProperty my0:cvIsConfidential ;
                    owl:onProperty my0:cvIsActive ;
                    owl:onProperty my0:aboutPerson ;
                    owl:onProperty my0:hasTarget ;
                    owl:cardinality "1"^^xsd:nonNegativeInteger ;
                    ];
  rdfs:comment "Curriculum Vitae (CV) Class"@en ;
  rdfs:label "CV"@en .

my0:cvLastUpdate
  a owl:DatatypeProperty ;
  rdfs:comment "Date when the information in CV was last updated."@en ;
  rdfs:label "Last Update"@en ;
  rdfs:domain my0:CV ;
  rdfs:range xsd:date .

my0:cvTitle
  a owl:DatatypeProperty ;
  rdfs:label "CV Title"@en ;
  rdfs:domain my0:CV ;
  rdfs:range xsd:string .

my0:cvNotes
  a owl:DatatypeProperty ;
  rdfs:comment "Notes about specific things the person wants to have known"@en ;
  rdfs:label "CV Notes"@en ;
  rdfs:domain my0:CV ;
  rdfs:range xsd:string .

my0:cvCopyright
  a owl:DatatypeProperty ;
  rdfs:comment "Copyright notice - who owns rights to CV, how may it be used, other legal info."@en ;
  rdfs:label "CV Copyright"@en ;
  rdfs:domain my0:CV ;
  rdfs:range xsd:string .

my0:cvIsConfidential
  a owl:DatatypeProperty ;
  rdfs:comment "If the CV is confidential and may be used by receiving party only."@en ;
  rdfs:label "Is CV Confidential"@en ;
  rdfs:domain my0:CV ;
  rdfs:range xsd:boolean .

my0:cvIsActive
  a owl:DatatypeProperty ;
  rdfs:comment "If the CV is active and the person is actively looking for job opportunities."@en ;
  rdfs:label "IS CV Active"@en ;
  rdfs:domain my0:CV ;
  rdfs:range xsd:boolean .

my0:aboutPerson
  a owl:ObjectProperty ;
  rdfs:label "About Person"@en ;
  rdfs:domain my0:CV ;
  rdfs:range my0:Person .

my0:CV_Entry
  a owl:Class ;
  rdfs:label "CV Entry"@en ;
  rdfs:comment "Single entry of CV information. Type of CV information specified in subclasses"@en ;
  rdfs:subClassOf rdfs:Resource .

my0:hasWorkHistory
  a owl:ObjectProperty ;
  rdfs:label "has Work History"@en ;
  rdfs:domain my0:CV ;
  rdfs:range my0:WorkHistory .

my0:hasEducation
  a owl:ObjectProperty ;
  rdfs:label "has Education"@en ;
  rdfs:domain my0:CV ;
  rdfs:range my0:Education .

my0:hasCourse
  a owl:ObjectProperty ;
  rdfs:label "has Course"@en ;
  rdfs:domain my0:CV ;
  rdfs:range my0:Course .

my0:hasSkill
  a owl:ObjectProperty ;
  rdfs:label "has Skill"@en ;
  rdfs:domain my0:CV ;
  rdfs:range my0:Skill .

my0:hasReference
  a owl:ObjectProperty ;
  rdfs:label "has Reference"@en ;
  rdfs:domain my0:CV ;
  rdfs:range my0:Reference .

my0:hasTarget
  a owl:ObjectProperty ;
  rdfs:label "has Target"@en ;
  rdfs:domain my0:CV ;
  rdfs:range my0:Target .

my0:hasOtherInfo
  a owl:ObjectProperty ;
  rdfs:label "has Other Info"@en ;
  rdfs:domain my0:CV ;
  rdfs:range my0:OtherInfo .

my0:Person
  a owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty my0:firstName ;
                    owl:onProperty my0:lastName ;
                    owl:onProperty my0:title ;
                    owl:onProperty my0:photo ;
                    owl:onProperty my0:gender ;
                    owl:onProperty my0:driversLicence ;
                    owl:onProperty my0:address ;
                    owl:onProperty my0:email ;
                    owl:onProperty my0:dateOfBirth ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                  ];
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty my0:hasNationality ;
                    owl:onProperty my0:hasCitizenship ;
                    owl:maxCardinality "2"^^xsd:nonNegativeInteger ;
                  ];
  rdfs:comment "Person Class that defines neccessary information about the specific person's curriculum vitae"@en ;
  rdfs:label "Person"@en ;
  rdfs:subClassOf dbo:Person .

my0:firstName
  a owl:DatatypeProperty ;
  rdfs:label "first name"@en ;
  rdfs:comment "Indicates the first name of the person"@en ;
  rdfs:domain my0:Person ;
  rdfs:range xsd:string .

my0:lastName
  a owl:DatatypeProperty ;
  rdfs:label "last name"@en ;
  rdfs:comment "Indicates the last name of the person"@en ;
  rdfs:domain my0:Person ;
  rdfs:range xsd:string .

my0:title
  a owl:ObjectProperty ;
  rdfs:label "title"@en ;
  rdfs:comment "Indicates the title of the person"@en ;
  rdfs:domain my0:Person ;
  rdfs:range mybase0:TitleProperty .

my0:photo
  a owl:DatatypeProperty ;
  rdfs:label "photo"@en ;
  rdfs:comment "Indicates the person's photo path"@en ;
  rdfs:domain my0:Person ;
  rdfs:range xsd:string .

my0:gender
  a owl:ObjectProperty ;
  rdfs:label "gender"@en;
  rdfs:comment "Gender property (man/woman/not indicated)"@en ;
  rdfs:domain my0:Person ;
  rdfs:range mybase0:SexProperty .

my0:hasCitizenship
  a owl:ObjectProperty ;
  rdfs:label "has Citizenship"@en ;
  rdfs:comment "Person's citizenship."@en ;
  rdfs:domain my0:Person ;
  rdfs:range country:ISO3166DefinedCountry .

my0:hasNationality
  a owl:ObjectProperty ;
  rdfs:comment "Person's nationality."@en ;
  rdfs:label "has Nationality"@en ;
  rdfs:domain my0:Person ;
  rdfs:range country:ISO3166DefinedCountry .

my0:driversLicence
  a owl:DatatypeProperty ;
  rdfs:label "drivers Licence"@en ;
  rdfs:comment "Indicates the person's drivers license."@en ;
  rdfs:domain my0:Person ;
  rdfs:range xsd:string .

my0:address
  a owl:ObjectProperty ;
  rdfs:label "address"@en ;
  rdfs:comment "Property that points towards the address of the person"@en ;
  rdfs:domain my0:Person ;
  rdfs:range my0:Address .

my0:Address
  a owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty my0:street ;
                    owl:onProperty my0:postalCode ;
                    owl:onProperty my0:city ;
                    owl:onProperty my0:country ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                  ];
  rdfs:label "Address"@en ;
  rdfs:comment "Indicates the full address object"@en .

my0:street
  a owl:DatatypeProperty ;
  rdfs:label "street"@en ;
  rdfs:comment "Indicates the street (name+code) in an address"@en ;
  rdfs:domain my0:Address ;
  rdfs:range xsd:string .

my0:postalCode
  a owl:DatatypeProperty ;
  rdfs:label "postal Code"@en ;
  rdfs:comment "Indicates the postal code in an address"@en ;
  rdfs:domain my0:Address ;
  rdfs:range xsd:string .

my0:country
  a owl:ObjectProperty ;
  rdfs:label "country"@en ;
  rdfs:comment "Indicates the country in an address"@en ;
  rdfs:domain my0:Address ;
  rdfs:range country:ISO3166DefinedCountry .

my0:city
  a owl:DatatypeProperty ;
  rdfs:label "city"@en ;
  rdfs:comment "Indicates the city in an address"@en ;
  rdfs:domain my0:Address ;
  rdfs:range xsd:string .

my0:email
  a owl:DatatypeProperty ;
  rdfs:label "email"@en ;
  rdfs:comment "Indicates the person's e-mail"@en ;
  rdfs:domain my0:Person ;
  rdfs:range xsd:string .

my0:hasPhoneNumber
  a owl:DatatypeProperty ;
  rdfs:label "phone Number"@en ;
  rdfs:comment "Indicates a telephone number of the person"@en ;
  rdfs:domain my0:Person ;
  rdfs:range xsd:string .

my0:hasWebsite
  a owl:DatatypeProperty ;
  rdfs:label "website"@en ;
  rdfs:comment "Indicates the url of a person's website, blog or linkedin"@en ;
  rdfs:domain my0:Person ;
  rdfs:range xsd:string .

my0:hasInstantMessaging
  a owl:ObjectProperty ;
  rdfs:label "has Instant Messaging"@en ;
  rdfs:comment "Property that points towards the instant messaging of a person"@en ;
  rdfs:domain my0:Person ;
  rdfs:range my0:InstantMessaging .

my0:InstantMessaging
  a owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty my0:instantMessagingName ;
                    owl:onProperty my0:instantMessagingUsername ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                  ];
  rdfs:label "Instant Messaging"@en ;
  rdfs:comment "Indicates the type and username of the instant messaging options of a person"@en .

my0:instantMessagingName
  a owl:DatatypeProperty ;
  rdfs:label "instant Messaging Name"@en ;
  rdfs:comment "Indicates the instant messaging app name"@en ;
  rdfs:domain my0:InstantMessaging ;
  rdfs:range xsd:string .

my0:instantMessagingUsername
  a owl:DatatypeProperty ;
  rdfs:label "instant Messaging Username"@en ;
  rdfs:comment "Indicates the instant messaging username"@en ;
  rdfs:domain my0:InstantMessaging ;
  rdfs:range xsd:string .

my0:dateOfBirth
  a owl:DatatypeProperty ;
  rdfs:label "date of birth"@en ;
  rdfs:comment "Indicates the person's date of birth"@en ;
  rdfs:domain my0:Person ;
  rdfs:range xsd:date .

my0:Organization
  a owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty my0:organizationName ;
                    owl:onProperty my0:organizationAddress ;
                    owl:onProperty my0:organizationDescription ;
                    owl:onProperty my0:organizationPhoneNumber ;
                    owl:onProperty my0:organizationWebsite ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                  ];
  rdfs:comment "General class for organizations"@en ;
  rdfs:label "Organization"@en ;
  rdfs:subClassOf rdfs:Resource .

my0:Company
  a owl:Class ;
  rdfs:comment "The object that holds a company's information."@en ;
  rdfs:label "Company"@en ;
  rdfs:subClassOf my0:Organization .

my0:companyIndustry
  a owl:DatatypeProperty ;
  rdfs:comment "Industry/Sector of the company"@en ;
  rdfs:label "company Industry"@en ;
  rdfs:domain my0:Company ;
  rdfs:range xsd:string .

my0:EducationalOrg
  a owl:Class ;
  rdfs:comment "The object that holds an educational organization's (university, ...) information"@en ;
  rdfs:label "Educational Organization"@en ;
  rdfs:subClassOf my0:Organization .

my0:organizationName
  a owl:DatatypeProperty ;
  rdfs:comment "Name of the organization"@en ;
  rdfs:label "organization Name"@en ;
  rdfs:domain my0:Organization ;
  rdfs:range xsd:string .

my0:organizationSize
  a owl:DatatypeProperty ;
  rdfs:comment "Size of the organization"@en ;
  rdfs:label "company Size"@en ;
  rdfs:domain my0:Organization ;
  rdfs:range mybase0:CompanySize.

my0:organizationAddress
  a owl:ObjectProperty ;
  rdfs:comment "Address of the organization. Not all properties of the address are required"@en ;
  rdfs:label "organization Address"@en ;
  rdfs:domain my0:Organization ;
  rdfs:range my0:Address .

my0:organizationDescription
  a owl:DatatypeProperty ;
  rdfs:comment "Description about the organization."@en ;
  rdfs:label "organization Description"@en ;
  rdfs:domain my0:Organization ;
  rdfs:range xsd:string .

my0:organizationPhoneNumber
  a owl:DatatypeProperty ;
  rdfs:comment "Organization phone number"@en ;
  rdfs:label "organization Phone Number"@en ;
  rdfs:domain my0:Organization ;
  rdfs:range xsd:string .

my0:organizationWebsite
  a owl:DatatypeProperty ;
  rdfs:comment "Organization home page"@en ;
  rdfs:label "organization Website"@en ;
  rdfs:domain my0:Organization ;
  rdfs:range xsd:string .

my0:WorkHistory
  a owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty my0:employedIn ;
                    owl:onProperty my0:startDate ;
                    owl:onProperty my0:endDate ;
                    owl:onProperty my0:jobTitle ;
                    owl:onProperty my0:jobDescription ;
                    owl:onProperty my0:careerLevel ;
                    owl:onProperty my0:jobMode ;
                    owl:onProperty my0:isCurrent ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                  ];
  rdfs:comment "CV entry for work history"@en ;
  rdfs:label "Work History"@en ;
  rdfs:subClassOf my0:CV_Entry .

my0:employedIn
  a owl:ObjectProperty ;
  rdfs:comment "The company where the person is/was employed."@en;
  rdfs:label "employed In"@en ;
  rdfs:range my0:Company ;
  rdfs:domain my0:WorkHistory .

my0:startDate
  a owl:DatatypeProperty ;
  rdfs:label "start Date"@en ;
  rdfs:comment "Start date of employment.Format: YYYY-MM-DD May contain only year or year and month."@en ;
  rdfs:domain my0:WorkHistory ;
  rdfs:range xsd:date .

my0:endDate
  a owl:DatatypeProperty ;
  rdfs:label "end Date"@en ;
  rdfs:comment "End date of employment.Format: YYYY-MM-DD May contain only year or year and month."@en;
  rdfs:domain my0:WorkHistory ;
  rdfs:range xsd:date .

my0:jobTitle
  a owl:DatatypeProperty ;
  rdfs:comment "Occupation or position held"@en ;
  rdfs:label "job Title"@en ;
  rdfs:domain my0:WorkHistory ;
  rdfs:range xsd:string .

my0:jobDescription
  a owl:DatatypeProperty ;
  rdfs:comment "Description of the main activities and responsibilities in the held position."@en ;
  rdfs:label "job Description"@en ;
  rdfs:domain my0:WorkHistory ;
  rdfs:range xsd:string .

my0:careerLevel
  a owl:ObjectProperty ;
  rdfs:comment "Career level for the position in company."@en;
  rdfs:label "career Level"@en ;
  rdfs:domain my0:WorkHistory ;
  rdfs:range mybase0:CVCareerLevel .

my0:jobMode
  a owl:ObjectProperty ;
  rdfs:comment "Type of the job: employee / contractor / intern"@en ;
  rdfs:label "job Mode"@en ;
  rdfs:domain my0:WorkHistory ;
  rdfs:range mybase0:CVJobMode .

my0:isCurrent
  a owl:DatatypeProperty ;
  rdfs:comment "Indicates if this is a current workplace."@en ;
  rdfs:label "is Current"@en ;
  rdfs:domain my0:WorkHistory ;
  rdfs:range xsd:boolean .

my0:Education
  a owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty my0:studiedIn ;
                    owl:onProperty my0:eduStartDate ;
                    owl:onProperty my0:eduGradDate ;
                    owl:onProperty my0:degreeType ;
                    owl:onProperty my0:eduDescription ;
                    owl:onProperty my0:isEduCurrent ;
                    owl:onProperty my0:eduMajor ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                  ];
  rdfs:comment "CV entry for education"@en ;
  rdfs:label "Education"@en ;
  rdfs:subClassOf my0:CV_Entry .

my0:studiedIn
  a owl:ObjectProperty ;
  rdfs:comment "Educational organization where person studied."@en ;
  rdfs:label "studied In"@en ;
  rdfs:domain my0:Education ;
  rdfs:range my0:EducationalOrg .

my0:isEduCurrent
  a owl:DatatypeProperty ;
  rdfs:comment "Indicates if this is a current education."@en ;
  rdfs:label "is Current"@en ;
  rdfs:domain my0:Education ;
  rdfs:range xsd:boolean .

my0:eduStartDate
  a owl:DatatypeProperty ;
  rdfs:label "education Start Date"@en ;
  rdfs:comment "Education start date (YYYY-MM-DD)May contain only year or year and month."@en ;
  rdfs:domain my0:Education ;
  rdfs:range xsd:date .

my0:eduGradDate
  a owl:DatatypeProperty ;
  rdfs:label "education Graduation Date"@en ;
  rdfs:comment "Education end (graduation) date (YYYY-MM-DD)May contain only year or year and month."@en ;
  rdfs:domain my0:Education ;
  rdfs:range xsd:date .

my0:degreeType
  a owl:ObjectProperty ;
  rdfs:label "degree Type"@en ;
  rdfs:comment "Title of qualification awarded"@en ;
  rdfs:domain my0:Education ;
  rdfs:range mybase0:EduDegree .

my0:eduMajor
  a owl:DatatypeProperty ;
  rdfs:comment "Major speciality graduated."@en ;
  rdfs:label "edu Major"@en ;
  rdfs:domain my0:Education ;
  rdfs:range xsd:string .

my0:eduMinor
  a owl:DatatypeProperty ;
  rdfs:comment "Minor speciality(-ies) graduated."@en ;
  rdfs:label "edu Minor"@en ;
  rdfs:domain my0:Education ;
  rdfs:range xsd:string .

my0:eduDescription
  a owl:DatatypeProperty ;
  rdfs:comment "Description of the main subjects / occupational skills covered."@en ;
  rdfs:label "edu Description"@en ;
  rdfs:domain my0:Education ;
  rdfs:range xsd:string .

my0:Course
  a owl:Class ;
  rdfs:subClassOf [ rdf:type owl:Restriction ;
                    owl:onProperty my0:hasCertification ;
                    owl:onProperty my0:courseTitle ;
                    owl:onProperty my0:courseDescription ;
                    owl:onProperty my0:courseURL ;
                    owl:onProperty my0:courseStartDate ;
                    owl:onProperty my0:courseFinishDate ;
                    owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                  ];
