  rdfs:domain my0:Course ;
  rdfs:range xsd:boolean .

my0:hasQualification
  a owl:ObjectProperty ;
  rdfs:label "has Qualification"@en ;
  rdfs:domain my0:Course ;
  rdfs:range esco:Qualification .

my0:courseTitle
  a owl:DatatypeProperty ;
  rdfs:comment "Title of the course taken or certification acquired."@en ;
  rdfs:label "course Title"@en ;
  rdfs:domain my0:Course ;
  rdfs:range xsd:string .

my0:courseDescription
  a owl:DatatypeProperty ;
  rdfs:comment "Description of the course / certification."@en ;
  rdfs:label "course Description"@en ;
  rdfs:domain my0:Course ;
  rdfs:range xsd:string .

my0:courseURL
  a owl:DatatypeProperty ;
  rdfs:label "course URL"@en ;
  rdfs:comment "Web page of the courses taken / certification acquired. May be pointer to certification transcript."@en ;
  rdfs:domain my0:Course ;
  rdfs:range xsd:string .

my0:courseStartDate
  a owl:DatatypeProperty ;
  rdfs:comment "Start date of the course taken."@en ;
  rdfs:label "course Start Date"@en ;
  rdfs:domain my0:Course ;
  rdfs:range xsd:date .

my0:courseFinishDate
  a owl:DatatypeProperty ;
  rdfs:comment "Date when course was finished / certification acquired."@en ;
  rdfs:label "course Finished Date"@en ;
