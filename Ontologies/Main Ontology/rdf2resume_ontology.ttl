  a owl:ObjectProperty ;
  rdfs:comment "Property regarding the content creation skills"@en ;
  rdfs:label "content Creation"@en ;
  rdfs:domain my0:DigitalSkills ;
  rdfs:range mybase0:SelfAssessmentProperty .

my0:safety
  a owl:ObjectProperty ;
  rdfs:comment "Property regarding the safety skills"@en ;
  rdfs:label "safety"@en ;
  rdfs:domain my0:DigitalSkills ;
